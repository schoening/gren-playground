module Model exposing (..)



type alias Flags =
    { folderName : String
    , files : Array File
    }


type alias File =
    { name : String
    , extension : String
    , content : String
    }


type FolderName
    = FolderName String
    | New


type alias Model =
    { sidebarWidth : Int
    , files : Array File
    , selectedFile : Maybe File
    , folderName : FolderName
    , reloadIframeHack : Int
    }
